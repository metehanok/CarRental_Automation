                                    @if (Session["Subeler"] != null)
                                    {
                                        var subeler = Session["Subeler"] as List<Subeler>;
                                        foreach (var sube in subeler)
                                        {
                                            <option value="@sube.sube_no">@sube.sube_ad</option>
                                        }
                                    }

                                </select>
                            </div>
                            <div class="col-md-6">
                                <label for="parkingNumber">Drop Location</label>
                                <select class="form-control" id="dropLocation" name="dropLocation">
                                    @if (Session["Subeler"] != null)
                                    {
                                        var subeler = Session["Subeler"] as List<Subeler>;
                                        foreach (var sube in subeler)
                                        {
